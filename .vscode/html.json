  "burger": {
    "prefix": "-burger",
    "body": [
      "<button class=\"burger\" aria-label=\"Открыть меню\" aria-expanded=\"false\"  data-burger>",
      "\t<span class=\"burger__line\"></span>",
      "</button>"
    ]
  },
  "checkbox": {
    "prefix": "-checkbox",
    "body": [
      "<label class=\"custom-checkbox\">",
      "\t<input type=\"checkbox\" class=\"custom-checkbox__field\">",
      "\t<span class=\"custom-checkbox__content\">$1</span>",
      "</label>"
    ]
  },
  "social": {
    "prefix": "-social",
    "body": [
      "<ul class=\"social\" title=\"$1\">",
      "\t<li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--fb\" aria-label=\"Наша страничка в Фейсбуке\"></a></li>",
      "\t<li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--vk\" aria-label=\"Наша страничка во Вконтакте\"></a></li>",
      "\t<li class=\"social__item\"><a href=\"#\" target=\"_blank\" class=\"social__link social__link--tw\" aria-label=\"Наша страничка в Твиттере\"></a></li>",
      "</ul>"
    ]
  },
  "basicform": {
    "prefix": "-form",
    "body": [
      "<form action=\"#\" class=\"form\">",
      "\t<label class=\"form__label\">",
      "\t\t<input type=\"text\" name=\"Имя\" class=\"input-reset form__input\" placeholder=\"Имя\">",
      "\t</label>",
      "\t<button class=\"btn-reset form__btn\">Отправить</button>",
      "</form>"
    ]
  },
  "modal button": {
    "prefix": "-modal-btn",
    "body": [
      "<button class=\"modal-btn $1\" data-path=\"$2\">$3</button>"
    ]
  },
  "modal structure": {
    "prefix": "-modal-structure",
    "body": [
      "<div class=\"modal\">",
      "\t<div class=\"modal__container\" role=\"dialog\" aria-modal=\"true\" data-target=\"$1\">",
      "\t\t<button class=\"modal-btn modal__close\" aria-label=\"Закрыть модальное окно\"></button>",
      "\t\t<div class=\"modal__content\">$2</div>",
      "\t</div>",
      "</div>"
    ]
  },
  "tooltip": {
    "prefix": "-tooltip",
    "body": [
      "<span class=\"tooltip\">",
      "\t<button class=\"tooltip__btn\" aria-describedby=\"$1\">$2</button>",
      "\t<span class=\"tooltip__txt\" role=\"tooltip\" id=\"$1\">",
      "\t\t<span class=\"visually-hidden\">Тултип: </span>",
      "\t\t$3",
      "\t</span>",
      "</span>"
    ]
  },
  "tabs sctructure": {
    "prefix": "-tabs",
    "body": [
      "<div class=\"tabs\" data-tabs=\"$1\">",
      "\t<ul class=\"tabs__list\">",
      "\t\t<li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$2</button></li>",
      "\t\t<li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$3</button></li>",
      "\t\t<li class=\"tabs__item\"><button class=\"tabs__btn\" type=\"button\">$4</button></li>",
      "\t</ul>",
      "\t<div class=\"tabs__content\">",
      "\t\t<div class=\"tabs__panel\">$5</div>",
      "\t\t<div class=\"tabs__panel\">$6</div>",
      "\t\t<div class=\"tabs__panel\">$7</div>",
      "\t</div>",
      "</div>"
    ]
  }
