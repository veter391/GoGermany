    "prefix": "-picture",
    "body": [
      "<picture class=\"$2\">",
      "\t<source srcset=\"@img/$1.webp 1x, @img/$1@2x.webp 2x\" type=\"image/webp\">",
      "\t<img loading=\"lazy\" class=\".lazy$2\" src=\"@img/$1.png\" srcset=\"@img/$1.png 1x, @img/$1@2x.png 2x\" width=\"$4\" height=\"$5\" alt=\"image\">",
      "</picture>"
    ]
  },
