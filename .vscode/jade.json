{
  "Pug picture": {
    "prefix": "-picture",
    "body": [
      "picture$2",
      "\tsource(srcset=\"@img/$1.webp 1x, @img/$1@2x.webp 2x\" type=\"image/webp\")",
      "\timg$2( src=\"@img/$1.png\" srcset=\"@img/$1.png 1x, @img/$1@2x.png 2x\" alt=\"image\")"
    ]
  },
  "Pug navigation": {
    "prefix": "-nav",
    "body": [
      "nav.nav",
      "\tul.nav__list",
      "\t\tli.nav__item",
      "\t\t\ta.nav__link(href=\"#\") $1",
      "\t\tli.nav__item",
      "\t\t\ta.nav__link(href=\"#\") $2",
      "\t\tli.nav__item",
      "\t\t\ta.nav__link(href=\"#\") $3"
    ]
  },
  "Pug unordered list": {
    "prefix": "-ul",
    "body": [
      "ul$1__list",
      "\tli$1__item",
      "\tli$1__item",
      "\tli$1__item"
    ]
  },
  "Pug ordered list": {
    "prefix": "-ol",
    "body": [
      "ol$1__list",
      "\tli$1__item",
      "\tli$1__item",
      "\tli$1__item"
    ]
  },
  "Pug swiper": {
    "prefix": "-swiper",
    "body": [
    ".swiper",
    "\t.swiper-wrapper",
    "\t\t.swiper-slide Slide 1",
    "\t\t.swiper-slide Slide 2",
    "\t\t.swiper-slide Slide 3",
    "\t.swiper-pagination",
    "\t.swiper-button-prev",
    "\t.swiper-button-next",
    "\t.swiper-scrollbar"
    ]
  },
  "Pug svguse": {
    "prefix": "-use",
    "body": [
      "svg",
      "\tuse(xlink:href=\"img/sprite.svg#$2\")"
    ]
  },
  "Pug burger": {
    "prefix": "-burger",
    "body": [
      "button.burger(aria-label=\"Открыть меню\" aria-expanded=\"false\"  data-burger)",
      "\tspan.burger__line"
    ]
  },
  "Pug checkbox": {
    "prefix": "-checkbox",
    "body": [
      "label.custom-checkbox",
      "\tinput.custom-checkbox__field(type=\"checkbox\")",
      "\tspan.custom-checkbox__content $1"
    ]
  },
  "Pug social": {
    "prefix": "-social",
    "body": [
      "ul.social(title=\"$1\")>",
      "\tli.social__item",
      "\t\ta.social__link.social__link--fb(href=\"#\" target=\"_blank\" aria-label=\"Наша страничка в Фейсбуке\")",
      "\tli.social__item",
      "\t\ta.social__link.social__link--vk(href=\"#\" target=\"_blank\" aria-label=\"Наша страничка во Вконтакте\")",
      "\tli.social__item",
      "\t\ta.social__link.social__link--tw(href=\"#\" target=\"_blank\" aria-label=\"Наша страничка в Твиттере\")"
    ]
  },
  "Pug basicform": {
    "prefix": "-form",
    "body": [
      "form.form(action=\"#\")",
      "\tlabel.form__label",
      "\t\tinput.input-reset.form__input(type=\"text\" name=\"Имя\" placeholder=\"Имя\")",
      "\tbutton.btn-reset.form__btn(type=\"submit\") Отправить"
    ]
  },
  "Pug modal button": {
    "prefix": "-modal-btn",
    "body": [
      "button.modal-btn(data-path=\"$2\") $3"
    ]
  },
  "Pug modal structure": {
    "prefix": "-modal-structure",
    "body": [
      ".modal",
      "\t.modal__container(role=\"dialog\" aria-modal=\"true\" data-target=\"$1\")",
      "\t\tbutton.modal-btn.modal__close(aria-label=\"Закрыть модальное окно\")",
      "\t\t.modal__content",
      "\t\t\t$2"
    ]
  },
  "Pug tooltip": {
    "prefix": "-tooltip",
    "body": [
      "span.tooltip",
      "\tbutton.tooltip__btn(aria-describedby=\"$1\") $2",
      "\tspan.tooltip__txt(role=\"tooltip\" id=\"$1\")",
      "\t\tspan.visually-hidden Тултип:",
      "\t\t\t$3"
    ]
  },
  "Pug tabs sctructure": {
    "prefix": "-tabs",
    "body": [
      ".tabs(data-tabs=\"$1\")",
      "\tul.tabs__list",
      "\t\tli.tabs__item",
      "\t\t\tbutton.tabs__btn(type=\"button\") $2",
      "\t\tli.tabs__item",
      "\t\t\tbutton.tabs__btn(type=\"button\") $3",
      "\t\tli.tabs__item",
      "\t\t\tbutton.tabs__btn(type=\"button\") $4",
      "\t.tabs__content",
      "\t\t.tabs__panel",
      "\t\t\t$5",
      "\t\t.tabs__panel",
      "\t\t\t$6",
      "\t\t.tabs__panel",
      "\t\t\t$7"
    ]
  }
}
